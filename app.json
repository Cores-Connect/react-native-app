{
  "expo": {
    "name": "MortyAIApp",
    "slug": "morty-ai-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.jpg",
    "userInterfaceStyle": "dark",
    "scheme": "mortyai",
    "entryPoint": "node_modules/expo/AppEntry.js",
    "splash": {
      "image": "./assets/app-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0A0F0F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.jpg",
        "backgroundColor": "#0A0F0F"
      },
      "package": "com.ahmad12345w2.mortyaiapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7bffac39-b70e-4436-a7e9-c692c54fd665"
      }
    }
  }
}
